<!DOCTYPE html>
<html>
<head>
    <title>myAnimeList</title>
    <link rel="stylesheet" href="http://localhost:3000/bg.css">
    <link rel="stylesheet" href="http://localhost:3000/box.css">


</head>
<style>
  .user_profile{
    position: absolute;
    top: 0;
    right: 0;
    text-align: justify;
    padding: 10px;
  }
</style>
<body style="color: white">
    <div class="wrapper">
      
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      <div><span class="dot"></span></div>
      
  </div>
  
  <div class="user_stuffs">
  <% if(locals.login_status == false) {%>
  
    <br>
    <button><a href="/login" style="color: white">LOGIN</a></button>  
    <button><a href="/register" style="color: white">REGISTER</a></button>
  <br><br>
  <% } %> <a href="/manga">
  <img height="70px" src="data:image/png;base64,"></a>
  
</div>

  <div class = "user_profile">
    
    
    <% if(locals.login_status == true) {%>
      <img class = "user_propic" src = <%=locals.pro_pic %> > <br>
      Hello, <p style = "color : red"><a href="/user/<%= locals.user_id %>" style="color: white"><%= locals.username %></a> !</p>
      <button><a href="/logout">LOGOUT</a></button>
      
    <% } %>
    
    <% if(locals.login_status == false) {%>
      <div class="tenor-gif-embed" data-postid="25682853" data-share-method="host" data-aspect-ratio="1.77778" data-width="100%"><a href="https://tenor.com/view/anya-gif-25682853">Anya GIF</a>from <a href="https://tenor.com/search/anya-gifs">Anya GIFs</a></div> <script type="text/javascript" async src="https://tenor.com/embed.js"></script><!-- <div class="tenor-gif-embed" data-postid="27052786" data-share-method="host" data-aspect-ratio="1.78771" data-width="100%"><a href="https://tenor.com/view/intense-anya-anya-forger-anime-anya-forger-anya-smile-gif-27052786">Intense Anya GIF</a>from <a href="https://tenor.com/search/intense-gifs">Intense GIFs</a></div> <script type="text/javascript" async src="https://tenor.com/embed.js"></script> -->
      Hello, <p style = "color : red ">Anya!</p><br>
    <% } %>
    
  </div>
  <div class="centered">
    <a href = "/"><img class="logo" src="http://localhost:3000/logo.png"></a><br><br><br>
      
    <form action="/manga/<%=manga_id%>/add/anime/search" method="get">
      <input type = "hidden" name = "page_index" value = "<%=page_index%>">       
      <input type="text" id="search" name="title" placeholder="Search Anime">
      <input type = "text" id = "search_genre" name = "genre" placeholder = "Search by Genre">
          <select id = "year" name = "year">
            <option value = "">Release Year</option>
            <%year.forEach(row =>{%>
              <option value = "<%= row[0]%>"><%= row[0]%></option>
            <% }); %>
          </select>
          <select id = "status" name = "status">
            <option value = "">Status</option>
            <option value = "Finished">Finished</option>
            <option value = "Releasing">Releasing</option>
          </select>
          <select id = "country" name = "country">
            <option value = "">Country of Origin</option>
            <%country_of_origin.forEach(row => {%>
              <option value = "<%= row[0]%>"><%= row[0]%></option>
            <% }); %>
          </select>
          <input type="checkbox" id = "manga_adapted" name = "manga_adapted"> Manga Adapted </input> 
          <input type= "text" id = "characters" name = "characters" placeholder="Character's Name">
          <input type = "text" id = "studio" name = "studio" placeholder= "Studio">
      <button type="submit">Search</button>
    </form>
    <br><br><br><br>

      <%data.forEach(row =>{%>
        <a href="/anime/<%=row[0]%>">
            <div class="media-placeholder">

                <div class="vertical-box">
                  <a href="/manga/<%=manga_id%>/add/anime/<%=row[0]%>"><img class="rounded-image" src="<%= row[3]%>" alt="No image available" <%if(row[4] == 'YES') {%> style="filter:blur(15px)" <%}%>></a>
                </div>
                
                <div class="title">
                  <%= row[1]%>
                  <% if(row[1] == null)  {%> <%= row[2] %> <%}%>
                </div>
                
            </div>
        </a>
        <% }); %>
          
    </div>


</body>
</html>
